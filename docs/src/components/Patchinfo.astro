---
import { Code } from "@astrojs/starlight/components";
const response = await fetch(
	"https://github.com/userdocs/qbt-workflow-files/releases/latest/download/dependency-version.json"
);

const data = await response.json();

const codeblock = `
qbt-build/patches/zlib/${data.zlib || "1.3.1"}
qbt-build/patches/iconv/${data.iconv || "1.18"}
qbt-build/patches/icu/${data.icu || "76-1"}
qbt-build/patches/openssl/${data.openssl || "3.2.1"}
qbt-build/patches/boost/${data.boost || "1.87.0"}
qbt-build/patches/libtorrent/${data.libtorrent_2_0 || "2.0.10"}
qbt-build/patches/double_conversion/${data.double_conversion || "3.3.0"}
qbt-build/patches/qtbase/${data.qt6 || "6.8.1"}
qbt-build/patches/qttools/${data.qt6 || "6.8.1"}
qbt-build/patches/qbittorrent/${data.qbittorrent || "5.0.3"}
`;

